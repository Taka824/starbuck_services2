<%= stylesheet_link_tag 'like', :media => "all" %>
<div class='mr10 float-right'>
  <%= link_to like_path(current_user.likes.find_by(post_id:post.id)), id: "js-bookmark-button-for-post-#{post.id}",class: 'float-right', method: :delete  do %>
    <i class="fas fa-heart red-color"></i>
    <span class="like-count"><%= post.likes.count%></span>
  <% end %>
</div>