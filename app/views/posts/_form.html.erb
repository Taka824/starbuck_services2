<%= form_with model: post, local: true do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :arrange_content %>
    <%= f.text_area :arrange_content, class: 'form-control', rows: 10 %>
  </div>
  <div class="form-group">
    <%= f.label :price %>
    <%= f.number_field :price, class: 'form-control', rows: 10 %>
  </div>
  <div class="form-group">
    <%= f.label :one_point %>
    <%= f.text_field :one_point, class: 'form-control', rows: 10 %>
  </div>
  <div class="form-group">
    <%= f.label :images %>
    <%= f.file_field :images, class: 'form-control', rows: 10, multiple: true %>
  </div>
  <div class="form-group" id="star">
    <%= f.label :evaluation,'評価 :', class:"" %>
    <%= f.hidden_field :evaluation, id: :review_star %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
  
<% end %>
<%= javascript_include_tag "star__post.js" %>

